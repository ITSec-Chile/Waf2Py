 <script>
 $(document).ready(function(){
   refreshTable();
 });

 function refreshTable(){
 $('#tableHolder').load('/Waf2Py/default/status', function(){
     setTimeout(refreshTable, 5000);
 });
 /*$('#reload_status').load('/Waf2Py/default/r_status', function(){
     setTimeout(refreshTable, 2000);
